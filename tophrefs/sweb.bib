@ARTICLE{klynch-2008a,
  author = {Lynch, K.M. and Schwartz, I.B. and Peng Yang and Freeman, R.A.},
  title = {Decentralized Environmental Modeling by Mobile Sensor Networks},
  journal = {Robotics, IEEE Transactions on},
  year = {2008},
  volume = {24},
  pages = {710 -724},
  number = {3},
  month = {june },
  abstract = {Cooperating mobile sensors can be used to model environmental functions
	such as the temperature or salinity of a region of ocean. In this
	paper, we adopt an optimal filtering approach to fusing local sensor
	data into a global model of the environment. Our approach is based
	on the use of proportional-integral (PI) average consensus estimators,
	whereby information from each mobile sensor diffuses through the
	communication network. As a result, this approach is scalable and
	fully decentralized, and allows changing network topologies and anonymous
	agents to be added and subtracted at any time. We also derive control
	laws for mobile sensors to move to maximize their sensory information
