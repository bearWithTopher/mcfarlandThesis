@INPROCEEDINGS{sborthwick-1994a,
  author = {Borthwick, Stephen and Durrant-Whyte, Hugh},
  title = {Dynamic localization of autonomous guided vehicles},
  booktitle = {IEEE {I}nternational {C}onference on {M}ultisensor {F}usion and {I}ntegration
	for {I}ntelligent {S}ystems},
  year = {1994},
  pages = {92--97},
  address = {Las Vegas, NV, USA},
  month = oct,
  comment = {navigation single vehicle land},
  abstract = {As Autonomous Guided Vehicles become increasingly established within
	a diverse range of applications, the need for efficient and flexible
	operation becomes apparent. Existing localization techniques have
	tended to offer either `move look update' operation, navigating from
	an a priori map, or continuous operation dependent upon artificial
	beacons placed within the environment. We describe an Extended Kalman
	Filter based navigation system which maintains a robust position
	estimate throughout continuous operation. In order to achieve dynamic
	operation, we exploit the recursive nature of the Kalman Filter and
	utilize the higher data acquisition rate offered by infra red scanning
	to obtain observations of the operational environment. In order to
	further enhance performance, the a priori map is segmented into an
	array of regions containing sub lists of features which can be rapidly
	matched with an observation, thus minimizing the computation overhead
	due to data association.},
  doi = {10.1109/MFI.1994.398452},
  file = {sborthwick_1994a.pdf:home/swebster/swebsrefs/papers/borthwick/sborthwick_1994a.pdf:PDF},
  keywords = {Automation - Motion control - Navigation systems - Robustness (control
	systems) - Computational complexity - Parameter estimation - Recursive
	functions - Sensor data fusion - Data acquisition - Kalman filtering
	- Algorithms, Automated Guided Vehicles (AGV) - Extended Kalman Filter
	- Dynamic localization - Data association},
  owner = {swebster},
  timestamp = {2006.06.06}
}

