@INPROCEEDINGS{iashokaraj-2002a,
  author = {Ashokaraj, Immanuel A.R. and Silson, Peter M.G. and Tsourdos, Antonios
	and White, Brian A.},
  title = {Implementation of an {A}daptive {EKF} to {M}ultiple {L}ow {C}ost
	{N}avigation {S}ensors in {W}heeled {M}obile {R}obots},
  booktitle = {Proceedings of the 7th {I}nternational {C}onference on {C}ontrol,
	{A}utomation, {R}obotics and {V}ision, {ICARCV} 2002},
  year = {2002},
  pages = {608--612},
  address = {Singapore, Singapore},
  month = {Dec},
  abstract = {The present aim of this research is to design a navigation sensor
	suite for a newly built mobile robot using low cost multiple sensors.
	A basic requirement for an autonomous mobile robot is its ability
	to localize itself accurately. This paper describes an accurate method
	for generating navigational data for a wheeled mobile robot. An Adaptive
	Extended Kalman Filter (AEKF) is used to fuse data from multiple
	low cost sensors. In order to estimate the spatial position of a
	wheeled robot, a combination of accelerometers, a rate gyroscope
	and two wheel encoders are used. The system discussed in this paper
	has more measurement sensors than system states and therefore the
	sensors give over-lapping, low-grade information affected by noise,
	bias, drift etc. The dynamics of the robot and sensor system are
	non-linear. Therefore an AEKF is used to estimate these overlapping
	low-grade measured sensor data and give the best possible estimate
	of the mobile robot position. The adaptive mechanism in this case
	uses the Riccatti Equation adaption. The basic idea is to change
	the Kalman Gain. This is done by changing the Process noise co-variance
	matrix adaptively. Simulations show an improved performance in the
	estimates from the AEKF when compared to the EKF.},
  doi = {10.1109/ICARCV.2002.1238493},
  file = {iashokaraj_2002a.pdf:home/swebster/swebsrefs/papers/ashokaraj/iashokaraj_2002a.pdf:PDF},
  keywords = {Kalman filtering - Sensors - Navigation - Riccati equations - Gyroscopes
	- Accelerometers - Integration - Data reduction, Extended Kalman
	Filter (EKF) - Rotary encoders - Adaptive Extended Kalman Filter
	(AEKF) - Inertial sensors - Encoder errors},
  owner = {swebster},
  timestamp = {2006.06.06}
}

@INPROCEEDINGS{pbaccou-2003a,
  author = {Baccou, P. and Jouvencel, B.},
  title = {Simulation results, post-processing experimentations and comparisons
	results for navigation, homing and multiple vehicles operations with
	a new positioning method using on transponder},
  booktitle = C-IROS,
  year = {2003},
  volume = {1},
  pages = {811--817},
  month = oct,
  comment = {navigation ranges sims},
  abstract = {Homing and navigation capabilities are essential for many autonomous
	underwater vehicle (AUV) applications. This paper presents both problems
	with respect to a single beacon and an extension of the method for
	multiple AUV operation using relative location. The difficulties
	of this approach are due to the fact that a single range measurement
	does not completely constrain the beacon's position in the vehicle
	frame. In order to triangulate his position, the AUV needs to maneuver
	while measuring its displacements between ranges. In addition, range
	measurements are noisy and sometimes spurious, speed bias and underwater
	currents affect dead-reckoning measurements. All operation need to
	have the same beginning. An initialization phase is necessary to
	obtain an initial estimate of the vehicle's location with respect
	to a fixed or moving beacon. These initial estimates are refined
	during the actual displacement towards the beacon by means of a Kalman
	filter. Two kinds of navigation are used so as to maximize the information
	matrix and to maintain an accurate absolute position. We also present
	post-processing results in comparison with LBL navigation and mission
	experimentation post-processing treatment. And then we applied our
	method for two vehicles operation.},
  file = {pbaccou-2003a.pdf:home/swebster/swebsrefs/papers/baccou/pbaccou-2003a.pdf:PDF},
  owner = {swebster},
  timestamp = {2009.11.27}
}

