  title = {At-sea calibration of a {USBL} underwater vehicle positioning system},
  booktitle = {Oceans Conference Record ({IEEE})},
  year = {1997},
  volume = {1},
  pages = {721--726},
  address = {Halifax, NS, Can},
  month = oct,
  abstract = {In this paper the at-sea calibration procedure for IFREMER's ultrashort
	baseline acoustic positioning system (USBL) POSIDONIA for deep-sea
	underwater vehicles (UV) is presented. The USBL system comprises
	an acoustic vessel-born localization system and sensors measuring
	vessel position, heading and attitude. The latter are used to transform
	the `acoustic' UV position into earth related coordinates. Misalignments
	between the different sensors lead to important positioning errors.
	Quick calibration of the USBL system at sea without additional hardware
	is therefore an important operational feature. The procedure presented
	here uses position measurements obtained with the uncalibrated system
	for a fixed-point moored transponder beacon. The absolute beacon
	coordinates are not known initially; they are identified, together
	with angular misalignments of the USBL sensors, by an iterative adaptive
	estimator. The vessel track during the recording of measurements
	is arbitrary and calibration is accomplished in less than one hour.
	Examples are shown to illustrate the USBL performances and the amelioration
	achieved with the calibrated system.},
  doi = {10.1109/OCEANS.1997.634455},
  issn = {0197-7385},
  keywords = {Calibration - Transponders - Underwater acoustics - Position measurement,
	Ultrashort baseline acoustic positioning system},
  pdf = {uuv_nav/Opderbecke1997.pdf}
}

